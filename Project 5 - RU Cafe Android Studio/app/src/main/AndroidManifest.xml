<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rutgerscafe">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.RutgersCafe">
        <activity
            android:name=".CoffeeActivity"
            android:label="Order Coffee"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".AddDonutToOrderActivity"
            android:label="Add Donut to Order"
            android:parentActivityName=".DonutActivity" />
        <activity
            android:name=".DonutActivity"
            android:label="Order Donuts"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".YourOrderActivity"
            android:label="Your Order"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".StoreOrdersActivity"
            android:label="Store Orders"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MainActivity">
            <intent-filter>
                <action
                    android:name="android.intent.action.MAIN" />
                <category
                    android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>